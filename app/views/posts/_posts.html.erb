<% posts.each do |post| %>
    <div class="card">
        <div class="card-body">
            <p>投稿者：<%= post.user.name %></p>
            <p>サークル名：<%= post.circle_name %></p>
            <p>大学名：<%= post.univ_name %></p>
            <p>活動内容<%= simple_format post.content %></p>
        </div>
    </div>
<% end %>